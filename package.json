{
  "name": "sagamatron",
  "description": "Take the boilerplate out of your React/Redux/Saga stack using the magic of TypeScript! ðŸ§™",
  "keywords": [
    "redux-saga"
  ],
  "version": "2.2.1",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agiledigital/sagamatron"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "type-coverage": "type-coverage --strict --detail --ignore-catch",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "test": "jest",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn publish",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/experimental-utils": "^3.6.1",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-total-functions": "^1.40.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "ts-jest": "^26.1.4",
    "type-coverage": "^2.9.0",
    "typescript": "^3.9.7"
  },
  "typeCoverage": {
    "atLeast": 99.38
  },
  "dependencies": {
    "flux-standard-action": "^2.1.1",
