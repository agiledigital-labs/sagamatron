    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^6.0.1",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.3.0",
    "typescript": "^3.6.3"
  },
  "typeCoverage": {
    "atLeast": 98.69
  },
  "dependencies": {
